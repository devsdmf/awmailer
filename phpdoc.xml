<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
    <title>AwMailer</title>
    <parser>
        <default-package-name>Iset</default-package-name><target>.</target>
        <encoding>utf8</encoding>
        <markers>
            <item>TODO</item>
            <item>FIXME</item>
        </markers>
        <extensions>
            <extension>php</extension>
            <extension>php3</extension>
            <extension>phtml</extension>
        </extensions>
    </parser>
    <transformer>
        <target>./web/docs/source</target>
    </transformer>
    <transformations>
        <template name="clean" />
    </transformations>
    <files>
        <directory>src</directory>
        <directory>app</directory>
        <ignore>app/Daemon.php</ignore>
        <ignore>app/Service.php</ignore>
        <ignore>bin/*</ignore>
        <ignore>db/*</ignore>
        <ignore>docs/*</ignore>
        <ignore>tests/*</ignore>
        <ignore>vendor/*</ignore>
        <ignore>web/*</ignore>
    </files>
</phpdoc>
